program = { SOI ~ datum* ~ EOI }
datum = _{ simple_datum | compound_datum }


simple_datum = _{ number | bool } 
number = { ASCII_DIGIT+ }
bool   = _{ BOOL_TRUE | BOOL_FALSE }

compound_datum = _{ proper_list }
proper_list = { L_PAREN ~ datum* ~ R_PAREN }

L_PAREN = _{ "(" }
R_PAREN = _{ ")" }
BOOL_TRUE = { "#true" | "#t" }
BOOL_FALSE = { "#false" | "#f" }